<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="MediaView">
        <attr name="imageLoadType" format="integer"/>
        <attr name="placeholder" format="integer"/>
    </declare-styleable>

    <declare-styleable name="MediaContainerView">
        <attr name="controlEnabled" format="boolean"/>
        <attr name="imageLoadType"/>
    </declare-styleable>

    <declare-styleable name="DescriptionInputView">
        <attr name="android:hint"/>
    </declare-styleable>

    <declare-styleable name="CommentListView">
        <attr name="listCountAtOnce" format="integer"/>
    </declare-styleable>

    <declare-styleable name="UserDataGridView">
        <attr name="listCountAtOnce"/>
    </declare-styleable>

    <declare-styleable name="UserPictureView">
        <attr name="allowsProfileOpen" format="boolean"/>
    </declare-styleable>
</resources>